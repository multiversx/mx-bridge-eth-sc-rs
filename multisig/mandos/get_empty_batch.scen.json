        {
            "step": "scQuery",
            "txId": "get-with-sc-query",
            "tx": {
                "to": "sc:multisig",
                "function": "getCurrentTxBatch",
                "arguments": []
            },
            "expect": {
                "out": [],
                "status": "0",
                "message": ""
            }
        }
    ]
}
