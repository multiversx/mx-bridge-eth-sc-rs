{
    "name": "get empty batch test - should return empty and not error",
    "steps": [
        {
            "step": "externalSteps",
            "path": "setup.scen.json"
        },
        {
            "step": "scCall",
            "txId": "get-with-sc-call",
            "tx": {
                "from": "address:user",
                "to": "sc:multisig",
                "function": "getCurrentTxBatch",
                "arguments": [],
                "gasLimit": "100,000,000",
                "gasPrice": "0"
            },
            "expect": {
                "out": [],
